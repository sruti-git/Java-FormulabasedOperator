import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("base:");
        int b=sc.nextInt();
        System.out.print("height:");
        int h=sc.nextInt();
        double result=0.5*b*h;
        System.out.print("area of triangle:"+result);
    }
}

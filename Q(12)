import java.util.Scanner;
public class Main{
    public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    System.out.println("Side:");
    int a=sc.nextInt();
    double result1=6*a*a;
    double result2=a*a*a;
    double result3=12*a;
    System.out.println("Surface of cube:"+result1);
    System.out.println("Volume of cube:"+result2);
    System.out.println("Perimeter of cube:"+result3);
    }
}

import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.print("length:");
        int l=sc.nextInt();
        System.out.print("breadth:");
        int b=sc.nextInt();
        int result=l*b;
        System.out.print("area of rectangle:"+result);
    }
}
